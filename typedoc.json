{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": [
		"src"
	], // ドキュメント生成対象のディレクトリ
	"entryPointStrategy": "expand", // srcディレクトリ内のファイルを展開
	"out": "tsdocs", // 出力先
	"includeVersion": true,
	"excludePrivate": true,
	"excludeInternal": true,
	"excludeExternals": true,
	"excludeNotDocumented": false,
	"sort": [
		"source-order"
	],
	"plugin": [
		"typedoc-plugin-markdown"
	], // Markdown形式にしたい場合
	"tsconfig": "tsconfig.json"
}
